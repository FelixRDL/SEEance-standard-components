  "dependencies": {
    "nodemon": "^2.0.4",
    "seeance-analysis-core": "^0.2.9",
    "rimraf": "^3.0.2",
    "simple-git": "^2.8.0"
  },
  "seeance": {
    "depends_on": [
      "blame",
      "commits",
      "files"
    ],
    "produces": [
      "blame",
      "commits",
      "files"
    ],
    "config_schema": {
      "extensions": {
        "type": "array",
        "description": "Ignore extensions",
        "default": ["jpg", "mp4", "wav", "docx"],
        "items": {
          "type": "string"
        }
      },
      "name_contains": {
        "type": "array",
        "description": "Ignore filenames containing",
        "default": ["package-lock"],
        "items": {
          "type": "string"
        }
      }
    },
    "type": "preprocessor"
  }
}
