{
  "name": "activity-over-time",
  "version": "0.0.0",
  "description": "Activity Over Time",
  "main": "lib/index.js",
  "scripts": {
    "dev": "nodemon test.js SEEance-test-repos zettelkasten --serve",
    "test": "node test.js SEEance-test-repos zettelkasten"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.19",
    "moment": "2.24.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "seeance-analysis-core": "^0.0.19",
    "simple-git": "^2.8.0"
  },
  "seeance": {
    "depends_on": [
      "commits"
    ],
    "ignorePreprocessors": [
      "remove-outlier-commits"
    ],
    "config_schema": {
      "start_date": {
        "description": "Count commits after:",
        "type":"string",
        "widget": "date"
      },
      "end_date": {
        "description": "Count commits until:",
        "type":"string",
        "widget": "date"
      },
      "time_frame": {
        "type": "string",
        "description": "Group activities by:",
        "widget": "radio",
        "oneOf": [
          {
            "description": "Hour",
            "enum": [
              "hour"
            ]
          },
          {
            "description": "Day",
            "enum": [
              "day"
            ]
          },
          {
            "description": "Week",
            "enum": [
              "week"
            ]
          },
          {
            "description": "Month",
            "enum": [
              "month"
            ]
          },
          {
            "description": "Year",
            "enum": [
              "year"
            ]
          }
        ],
        "default": "week"
      }
    },
    "type": "analysis",
    "category": "Collaboration"
  }
}
